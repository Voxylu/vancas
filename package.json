{
  "name": "vancas",
  "version": "1.0.1",
  "description": "A nice wrapper for the html canvas.",
  "main": "dist/cjs/index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:es": "tsc --module esnext --outDir dist/es",
    "build": "npm-run-all clean --parallel build:*",
    "prepublish": "npm-run-all build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Voxylu/vancas.git"
  },
  "keywords": [
    "canvas",
    "html",
    "wrapper",
    "game",
    "p5",
    "processing"
  ],
  "author": "Voxylu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Voxylu/vancas/issues"
  },
  "homepage": "https://github.com/Voxylu/vancas#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.3333"
  }
}
